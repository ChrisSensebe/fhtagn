extends adminLayout

block content
    .container
        .row
            .col-md-12
                h2.page-header #{pageTitle}
                - each user in users
                    h3
                        a(href="/admin/user/#{user._id}")= user.username
                    p= user.email
                    p= user.role
    .container
        .row
            .col-md-12
                a(href="/admin/newUser") create new user